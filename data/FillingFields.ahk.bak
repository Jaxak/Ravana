Doki(){
	Sleep, 100
	Array:=[ArrayToStr(GeneratorFIO()), "скриптовой доверенности от 01.09.2018 №262/2018", GeneratorPost(), ArrayToStr(GeneratorFIO()), "Справедливого решения верховных жрецов", GeneratorPost(), GeneratorPost(), "С.О. Прист", "Строка для адреса коробки в которой проживает", GeneratorBankAccount(), 0, GeneratorPost(), "Х.К. Равана"]
	Loop, 13 {
		SelectString()
		If (a_index=9){
			sleep, 300
			Send, ^a
		}
		If (a_index=10){
			sleep, 300
			Send, ^a
			if(EtoKpp(Take())){
				Send, {TAB}
			}
		}
		a:=Take()
		If(a!=""){
			Array[A_Index]:=a
		}
		If(Caps()){
			Array[A_Index]:=SwithRegistr(Array[A_Index])
		}
		
		Send % Array[A_Index]
		Send {Tab}
	}
Return
}
